package main

import "fmt"

//Escreva um programa que receba uma string e verifique se
//ela é um palíndromo. Informe ao usuário se é ou não.

func main() {
	var reverse string
	var x string

	for i := len(x) - 1; i > 0; i-- {
		reverse = reverse + string(x[i])

	}
	if x == reverse {
		fmt.Println("É palidromo")
	} else {
		fmt.Println("Não é palidromo")
	}
}
